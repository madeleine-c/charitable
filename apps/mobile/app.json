{
  "expo": {
    "name": "Charitable",
    "slug": "charitable",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "charitable",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.charitable.app"
    },
    "android": {
      "package": "com.charitable.app"
    },
    "web": {
      "bundler": "metro",
      "output": "static"
    },
    "plugins": ["expo-router", "expo-secure-store"],
    "experiments": {
      "typedRoutes": true
    }
  }
}
